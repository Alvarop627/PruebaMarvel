<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ejemplo.marvel.prueba">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.ejemplo.marvel.prueba.PruebaMarvel"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.PruebaMarvel">
        <activity
            android:name="com.ejemplo.marvel.prueba.ui.comicslist.ComicListActivity"
            android:exported="true" />
        <activity
            android:name="com.ejemplo.marvel.prueba.ui.comicdetail.ComicDetailActivity"
            android:exported="true" />
        <activity
            android:name="com.ejemplo.marvel.prueba.ui.home.HomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ejemplo.marvel.prueba.ui.character.CharacterListActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>